<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="true">
    <set>
        <objectAnimator android:propertyName="translationZ" android:duration="@android:integer/config_shortAnimTime" android:valueFrom="0" android:valueTo="20dp" android:valueType="floatType" />
    </set>
</item>

<item>
    <set>
        <objectAnimator android:propertyName="translationZ" android:duration="100" android:valueTo="0" android:valueType="floatType" />
    </set>
</item>
</selector>
